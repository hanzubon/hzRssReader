{
  "name": "hzRssReader",
  "version": "0.0.3",
  "description": "RSS Reader by HANZUBON",
  "main": "server/app.js",
  "license": "AGPLv3",
  "scripts": {
    "start": "npm run prebuild && concurrently --kill-others \"npm run server:run\" \"ng serve --port=4203 --aot --progress=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "e2e:client": "ng e2e",
    "prebuild": "ts-node --compiler-options='{\"module\": \"commonjs\"}' utils/git.version.ts ",
    "build": "ng build --prod --source-map=false --aot --service-worker --output-path=dist/client && npm run server:build && npm run worker:build",
    "demo:build": "npm run prebuild && ng build --configuration=demo --source-map=false --aot --service-worker --output-path=dist/client-demo",
    "server:run": "nodemon",
    "server:build": "tsc -p ./server",
    "worker:run": "ts-node --project worker/tsconfig.json --compiler-options='{\"module\": \"commonjs\"}' worker/worker.ts",
    "worker:build": "tsc -p ./worker",
    "ttrss_import:run": "ts-node --project worker/tsconfig.json --compiler-options='{\"module\": \"commonjs\"}' utils/ttrss_import.ts"
  },
  "author": "ISHIKAWA Mutsumi",
  "repository": {
    "url": "https://github.com/hanzubon/hzrss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.1",
